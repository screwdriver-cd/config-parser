{
    "annotations": {},
    "jobs": {
        "main": [
            {
                "image": "node:18",
                "commands": [
                    {
                        "name": "config-parse-error",
                        "command": "echo 'Error: Annotation \"screwdriver.cd/buildCluster\": doesnotexist is not a valid build cluster'; exit 1"
                    }
                ],
                "secrets": [],
                "environment": {
                    "SD_SKIP_REPOSITORY_CLONE": "true"
                }
            }
        ]
    },
    "workflowGraph": {
        "nodes": [
            {
                "name": "~pr"
            },
            {
                "name": "~commit"
            },
            {
                "name": "main"
            },
            {
                "name": "~pr:/.*/"
            }
        ],
        "edges": [
            {
                "src": "~pr",
                "dest": "main"
            },
            {
                "src": "~commit",
                "dest": "main"
            },
            {
                "src": "~pr:/.*/",
                "dest": "main"
            }
        ]
    },
    "errors": [
        "Error: Annotation \"screwdriver.cd/buildCluster\": doesnotexist is not a valid build cluster"
    ]
}
