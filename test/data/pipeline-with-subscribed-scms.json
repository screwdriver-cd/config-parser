{
    "annotations": {},
    "parameters": {},
    "jobs": {
        "main": [
            {
                "annotations": {},
                "secrets": [],
                "settings": {},
                "environment": {},
                "image": "node:6",
                "requires": ["~commit", "~pr", "~subscribe"],
                "commands": [
                    {
                        "name": "install",
                        "command": "npm install"
                    }
                ]
            }
        ]
    },
    "workflowGraph": {
        "nodes": [
