            { "src": "~pr", "dest": "main" },
            { "src": "~subscribe", "dest": "main" }
        ]
    },
    "subscribe": {
        "scmUrls": [{"git@github.com:supra08/quickstart-generic.git#master": ["~commit", "~tags", "~release"]}]
    }
