{
    "annotations": {},
    "jobs": {
        "main": [
            {
                "annotations": {},
                "image": "node:4",
                "commands": [
                    {
                        "name": "install",
                        "command": "npm install"
                    }
                ],
                "environment": {
                    "ARRAY": "[\"a\",\"b\"]",
                    "BOOL": "true",
                    "OBJECT": "{\"a\":\"cat\",\"b\":\"dog\"}",
                    "NUMBER": "3",
                    "DECIMAL": "3.1415927",
                    "STRING": "test"
                },
                "secrets": [],
                "settings": {},
                "requires": [
                    "~pr",
                    "~commit"
                ]
            }
        ]
    },
    "prChain": false,
    "workflowGraph": {
        "nodes": [
            { "name": "~pr" },
            { "name": "~commit" },
            { "name": "main" }
        ],
        "edges": [
            { "src": "~pr", "dest": "main" },
            { "src": "~commit", "dest": "main" }
        ]
    }

}
