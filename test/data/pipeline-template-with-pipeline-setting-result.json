{
    "annotations": {
        "foo": "a",
        "bar": "b"
    },
    "childPipelines": {
        "scmUrls": [
        "git@github.com:org/repo.git",
        "https://github.com:org/repo2.git"
        ],
        "startAll": true
    },
    "parameters": {
        "color": [
        "red",
        "blue"
        ]
    },
    "jobs": {
        "main": [{
            "annotations": {},
            "cache": {
                "pipeline": [
                    "node_modules/",
                    "~/.sbt"
                ],
                "event": [
                    "target/generated-intermediate-resouce/*"
                ],
                "job": [
                    "target/some-artifact.zip"
                ]
            },
            "image": "node:18",
            "commands": [
                {
                    "name": "init",
                    "command": "npm install"
                },
                {
                    "name": "test",
                    "command": "npm test"
                }
            ],
            "environment": {
                "BAR": "bar",
                "FOO": "foo",
                "SD_PIPELINE_TEMPLATE_FULLNAME": "foo/bar",
                "SD_PIPELINE_TEMPLATE_NAME": "bar",
                "SD_PIPELINE_TEMPLATE_NAMESPACE": "foo",
                "SD_PIPELINE_TEMPLATE_VERSION": "1.0.0"
            },
            "secrets": [
                "NPM_TOKEN"
            ],
            "settings": {},
            "requires": [
                "~pr",
                "~commit"
            ]
        }]
    },
    "workflowGraph": {
        "nodes": [
            { "name": "~pr" },
            { "name": "~commit" },
            { "name": "main" }
        ],
        "edges": [
            { "src": "~pr", "dest": "main" },
            { "src": "~commit", "dest": "main" }
        ]
    },
    "subscribe": {
        "scmUrls": [
            {
            "git@github.com:foo/bar.git#master": [
                "~commit",
                "~tags",
                "~release"
            ]
            }
        ]
    },
    "templateVersionId": 111
}